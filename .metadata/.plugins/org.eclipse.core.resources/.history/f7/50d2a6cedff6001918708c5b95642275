int employee_setId(Employee* this,int id)
{
	int retorno = -1;
	if(this!=NULL && isValidId(id)){
		this->id = id;
		retorno = 0;
	}
	return retorno;
}

int employee_getId(Employee* this,int* id)
{
	int retorno = -1;
	if(this!=NULL && id!=NULL)
	{
		retorno = 0;
		*id = this->id;
	}
	return retorno;
}

static int isValidId(int id)
{
	int retorno = 0;
	if(id >= 0){
		retorno = 1;
	}
	return retorno;
}
